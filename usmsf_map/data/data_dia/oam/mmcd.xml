<?xml version="1.0" encoding="UTF-8"?>

<mmcd_conf>
    <uasock socket_buffer_size="262144"  server_ssl="false" client_ssl="false" />
    <mmc_user replication="false" />

	<cmd_timeout>300</cmd_timeout>
	<port>
		<mmc>1003</mmc>
	</port>

	<dblog enabled="true" period="60" size_kbyte="1024" path="$(UASYS_HOME)/log/dbdata/mmclog" />
	<prnlib_list>
		<prnlib>$(UASYS_HOME)/lib/libuaprn.so</prnlib>
	</prnlib_list>
	<messhand_handler using_master_handler="false"> 
		<name>messhand</name>
	</messhand_handler>
</mmcd_conf>
