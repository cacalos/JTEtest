    </measure>
    <alarm_list>
      <alarm id="0" code="1001010" name="CPU_USAGE" field_id="0" enabled="true" masked="false" standby_disabled="false" non_busy_hour="false">
        <threshold unit="%">
          <minor value="70" times="3"/>
          <major value="80" times="3"/>
          <critical value="90" times="3"/>
        </threshold>
      </alarm>
    </alarm_list>
    <statistics enabled="true" period="5M" using_function="false">
      <item field_id="0" type="AVG"/>
