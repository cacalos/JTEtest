<?xml version="1.0" encoding="UTF-8"?>
<omsib_conf>
	<rts_file enabled="true" path="$(UASYS_HOME)/log/omsib/rts" />
	<sqlite write_db="true" dbpath="$(UASYS_HOME)/data/oam/rts" table="OAM_RTS_TABLE" />

	<uasock socket_buffer_size="262144" server_ssl="false" client_ssl="false" />
	<uasamd restart="true" kill_by_heartbeat="true" />

	<call_trace enabled="true" >
		<log_file max_log_size="1000000" max_log_file_count="10" />
		<trace_udp enabled="false" udp_port="1007" />
		<msg_buffering enabled="true" max_count="1000" max_send_count="100" />
	</call_trace>

	<console_msg>
		<console_dblog enabled="true" name="conlog" />
		<service_fault_dblog enabled="true" name="svclog" />
		<service_fault enabled="true" name="svcfault" />
		<msg_buffering enabled="true" max_count="1000" max_send_count="100" />
	</console_msg>

	<port>
		<status>1001</status>
		<trace>1002</trace>
	</port>

	<rtslib_list>
		<rtslib>$(UASYS_HOME)/lib/libuarts.so</rtslib>
		<rtslib>$(UASYS_HOME)/lib/libuarts_esme.so</rtslib>
	</rtslib_list>

	<rts_list>
		<rts id="171" name="UASAMD_STATUS" interval="5" />
		<rts id="512" name="CONSOLE_MSG"  interval="10" />
		<rts id="528" name="TRACE_MSG"  interval="10" />
		<rts id="20001" name="ALARM_LIST"  interval="5" />
		<rts id="20002" name="CURRENT_ALARM"  interval="5" />
		<rts id="20021" name="ESME_STATUS" interval="10" />
		<rts id="20100" name="MMC_AUTH"  interval="5" />
	    <rts id="20200" name="TRACE_LIST"  interval="5" />
	</rts_list>
</omsib_conf>
