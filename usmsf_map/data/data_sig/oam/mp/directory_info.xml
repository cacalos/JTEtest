<?xml version="1.0" encoding="UTF-8"?>
<class_list>
  <class name="DIR_INFO" id="14.1" enabled="true" alarm="true" stat="false" min_stat="false" hour_stat="false" day_stat="false" real_time="false">
    <measure interval="5">
      <field id="0" type="string" name="PATH"/>
      <field id="1" type="integer" name="FILE_COUNT"/>
      <field id="2" type="integer" name="DIR_COUNT"/>
      <field id="3" type="integer" name="LAST_TIME"/>
      <field id="4" type="integer" name="OLDEST_TIME"/>
      <field id="5" type="integer" name="TOTAL_SIZE"/>
      <field id="6" type="integer" name="NOW_TIME"/>
      <field id="7" type="calculation" name="NO_UPDATE_TIME" expression="0:6-0:3" />
    </measure>
    <alarm_list>
      <alarm id="0" code="1401010" name="FILE_COUNT" field_id="1" enabled="true" masked="false" standby_disabled="false" non_busy_hour="false">
        <threshold unit="%">
          <minor value="100" times="1"/>
          <major value="200" times="1"/>
          <critical value="500" times="1"/>
        </threshold>
      </alarm>
