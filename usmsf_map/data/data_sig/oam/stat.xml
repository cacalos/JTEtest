<?xml version="1.0" encoding="UTF-8"?>

<stat_config>
	<stat_group name="LOGSTAT" shm_name="LOG_STAT" sem_name="LOG_STAT" >
		<section>
			<small  max_record="10"  max_table="30"/>
			<medium max_record="50" max_table="50"/>
			<large  max_record="256" max_table="10" />
		</section>

        <stat name="NTP_STATUS" alarminfo_changed="false" accumulated_value="false" section_type="small" key_delimiter=";" thread_safety="true">
            <field_list>
                <field id="0" type="integer" name="STATUS" />
            </field_list>
            <stat_file append="false" accumulated_value="true" interval="5" path="$(UASYS_HOME)/data/oam/logstat/ntp_status.sts" delimeter=" ,&#9;&#10;" end_phrase="END_DATA" >
                <item column="0" field_id="0" >
                    <replace_list default_target="0">
                        <replace source="OK" target="1" />
                    </replace_list>
                </item>
            </stat_file>
        </stat>

