	schedule_timer
	dia_util
	dia_avp_util
	worker_util
	trc_util
    ;



ObjectCcFlags $(LIB_SRCS)$(SUFOBJ) : $(LOCAL_DEFINES) $(SYS_CCFLAGS) $(SYS_SHAREDCCFLAGS) ;
Library $(LIB_NAME)$(SUFLIB) : $(LIB_SRCS).c ;
SharedLibraryMain $(LIB_NAME)$(SUFSHARED) : $(LIB_NAME)$(SUFLIB) ;

